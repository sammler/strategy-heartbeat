{
  "name": "strategy-heartbeat",
  "version": "0.1.0",
  "description": "Basic heartbeat service (as a sammler strategy).",
  "keywords": [
    "sammler"
  ],
  "homepage": "https://github.com/sammler/strategy-heartbeat#readme",
  "bugs": {
    "url": "https://github.com/sammler/strategy-heartbeat/issues"
  },
  "license": "MIT",
  "author": "Stefan Walther <swr.nixda@gmail.com> (http://qliksite.io)",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sammler/strategy-heartbeat.git"
  },
  "scripts": {
    "d-build": "docker build -t sammlerio/strategy-heartbeat .",
    "d-run": "docker run -it sammlerio/strategy-heartbeat",
    "dc-down": "docker-compose --f=./docker-compose.yml down",
    "dc-up": "docker-compose --f=./docker-compose.yml up",
    "dc-up:b": "docker-compose --f=./docker-compose.yml up --build",
    "dc-up:deps": "docker-compose --f=./docker-compose.deps.yml up --build",
    "docs": "docker run --rm -v ${PWD}:/opt/verb stefanwalther/verb",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:fix": "npm run lint:src:fix && npm run lint:test:fix",
    "lint:src": "eslint src",
    "lint:src:fix": "eslint src --fix",
    "lint:test": "eslint test",
    "lint:test:fix": "eslint test --fix",
    "start": "node index.js",
    "start:debug": "node --inspect index.js",
    "start:watch": "(npm list nodemon -g || npm install nodemon -g) && nodemon -L --config ./nodemon.json --exec npm run start:debug",
    "test": "npm run lint && npm run test:integration",
    "test:integration": "mocha './test/integration/**/*.spec.js' --require './test/mocha.conf.js' --timeout 6000",
    "test:unit": "node ./node_modules/mocha/bin/_mocha './test/unit/**/*.spec.js' --require './test/mocha.conf.js'"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "body-parser": "^1.18.2",
    "death": "^1.1.0",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "http-status-codes": "^1.3.0",
    "js-yaml": "^3.10.0",
    "lepus": "^0.1.2",
    "lodash": "^4.17.4",
    "pkg": "^4.3.0-beta.1",
    "promise-retry": "^1.1.1",
    "winster": "^0.2.7"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "eslint": "4.19.1",
    "eslint-config-space-single": "0.3.3",
    "mocha": "4.0.1",
    "sinon": "4.5.0"
  }
}
